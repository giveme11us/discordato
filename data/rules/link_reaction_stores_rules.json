{
  "luisaviaroma": {
    "enabled": true,
    "name": "LUISAVIAROMA",
    "description": "Extract product IDs from LUISAVIAROMA embeds",
    "file_path": "/root/monitor/luisaviaroma/drop/urls.txt ",
    "detection": {
      "type": "author_name",
      "value": "LUISAVIAROMA"
    },
    "extraction": {
      "primary": "url",
      "pattern": "\\/[^\\/]+\\/([^\\/]+)$",
      "fallback": "field_pid"
    }
  }
}